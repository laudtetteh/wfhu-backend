{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "pages",
  "info": {
    "name": "page",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": "",
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true
    },
    "slug": {
      "type": "string",
      "unique": true
    },
    "publish_at": {
      "type": "datetime"
    },
    "seo": {
      "type": "component",
      "component": "page.seo",
      "required": false,
      "repeatable": false
    },
    "previous_": {
      "private": true,
      "type": "json"
    },
    "author_": {
      "private": true,
      "type": "json"
    },
    "dynamic_fields": {
      "type": "dynamiczone",
      "components": [
        "page.no-of-posts-to-show",
        "page.intro-cta",
        "page.connect-cta"
      ]
    },
    "description": {
      "type": "richtext"
    }
  }
}
