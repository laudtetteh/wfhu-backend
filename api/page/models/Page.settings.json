{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "pages",
  "info": {
    "name": "page",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": "",
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "slug": {
      "type": "string",
      "unique": false
    },
    "publish_at": {
      "type": "datetime"
    },
    "seo": {
      "type": "component",
      "component": "page.seo",
      "required": false,
      "repeatable": false
    },
    "dynamic_fields": {
      "type": "dynamiczone",
      "components": [
        "page.single-image"
      ]
    },
    "description": {
      "type": "richtext"
    }
  }
}
